doctype html
head
  meta(charset='UTF-8')
  meta(name='viewport' content='width=device-width, initial-scale=1.0')
  title Document
  link(href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css' rel='stylesheet')
form
form(action-"contact"method-"POST")
  .form-row
    .form-group.col-md-6
      label(for='julie@gmail.com') Email
      input#inputEmail4.form-control(type='email' placeholder='Email')
    .form-group.col-md-6
      label(for='1234') Password
      input#inputPassword4.form-control(type='password' placeholder='Password')
  .form-group
    label(for='kabalagala') Address
    input#inputAddress.form-control(type='text' placeholder='1234 Main St')
  .form-group
    label(for='kampala') Address 2
    input#inputAddress2.form-control(type='text' placeholder='Apartment, studio, or floor')
  .form-row
    .form-group.col-md-6
      label(for='kampala') City
      input#inputCity.form-control(type='text')
    .form-group.col-md-4
      label(for='inputState') State
      select#inputState.form-control
        option(selected='') kampala
        option buloba
        option namuera
        option wakiso
    .form-group.col-md-2
      label(for='inputZip') Zip
      input#inputZip.form-control(type='text')
  .form-group
    .form-check
      input#gridCheck.form-check-input(type='checkbox')
      label.form-check-label(for='gridCheck')
        | Check me out
  button.btn.btn-primary(type='submit') Sign in
script(src='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js')
